<div class="videos">
<% @videos.each do |video| %>
  <div class="listagem">
    <h2><%= link_to video.nome, video %> - <%= video.descricao %></h2>
    <%= link_to "Edit", edit_video_path(video) %> |
    <%= link_to "Destroy", video, :method => :delete, :confirm => "Are you sure?" %>
  </div>
<% end %>
</div>
</br>
<%= link_to 'New video', new_video_path, :class => "new" %>