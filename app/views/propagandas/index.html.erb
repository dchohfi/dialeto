<h1>Listing propagandas</h1>

<table>
  <tr>
    <th>Imagem</th>
    <th>Nome</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @propagandas.each do |propaganda| %>
  <tr>
    <td><%= propaganda.nome %></td>
    <td><%= propaganda.image.name %></td>
    <td><%= link_to 'Show', propaganda %></td>
    <td><%= link_to 'Edit', edit_propaganda_path(propaganda) %></td>
    <td><%= link_to 'Destroy', propaganda, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Propaganda', new_propaganda_path %>
